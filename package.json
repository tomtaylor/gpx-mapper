{
  "name": "gpx-mapper",
  "version": "1.0.2",
  "description": "Convert a directory of GPX files into a static website with an interactive map",
  "main": "src/index.js",
  "bin": {
    "gpx-mapper": "src/index.js"
  },
  "type": "module",
  "scripts": {
    "build": "node src/index.js ./gpx ./dist --title \"Route Map\""
  },
  "keywords": [
