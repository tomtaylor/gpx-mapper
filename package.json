    "routes",
    "maplibre",
    "pmtiles"
  ],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "commander": "^12.1.0",
    "gpxparser": "^3.0.8",
    "he": "^1.2.0"
  }
}
